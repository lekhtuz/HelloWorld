          "pattern" : "%d{dd HH:mm:ss,SSS} %-5level %logger{1.} - %msg%n"
        }
      }
    }, "loggers" : {
      "root" : {
        "level" : "debug"
      , "AppenderRef" : [
          { "ref" : "Stdout" }
        ]
      }
    }
  }
}